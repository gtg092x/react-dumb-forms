{
  "name": "react-dumb-forms",
  "version": "0.0.1",
  "description": "Deterministic Forms toolset for React",
  "main": "lib/react-dumb-forms.js",
  "scripts": {
    "test": "make test-cov",
    "gh-page": "./node_modules/.bin/pug index.pug",
    "compile": "./node_modules/.bin/babel --source-maps inline --optional runtime -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "stage-2",
      "react"
    ]
  },
  "nyc": {
    "exclude": [
      "*.js",
      "lib/*.js",
      "coverage/*.js",
      "**/__tests__/*.js",
      "**/__tests__/fixtures/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gtg092x/react-dumb-forms.git"
  },
  "keywords": [
    "forms",
    "react",
    "react-dumb-forms"
  ],
  "author": "gtg092x",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gtg092x/react-dumb-forms/issues"
  },
  "peerDependencies": {
    "react": "^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-prometheusresearch": "^0.2.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "jsdom": "^6.3.0",
    "mocha": "^2.3.2",
    "node-libs-browser": "^1.0.0",
    "nyc": "^6.1.1",
    "power-assert": "^1.3.1",
    "pug": "^2.0.0-beta3",
    "pug-cli": "^1.0.0-alpha6",
    "react": "^15.1.0",
    "sinon": "^1.16.1",
    "tape": "^2.13.4",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "dot-object": "^1.4.1",
    "lodash": "^3.10.1"
  }
}
